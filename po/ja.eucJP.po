#: bin/main.cc:107
#, c-format
msgid "Usage: %s [OPTION]...\n"
msgstr "使用法: %s [オプション]...\n"

#: bin/main.cc:108
msgid "Display a rotating 3D clock.\n"
msgstr "自転する3D時計を表示します。\n"

#: bin/main.cc:110
msgid "  -m, --hide-menu-bar   hide the menu bar\n"
msgstr "  -m, --hide-menu-bar   メニューバーを隠す\n"

#: bin/main.cc:111
msgid "      --private-colormap allocate a private colormap\n"
msgstr "      --private-colormap プライベートカラーマップを割り当てる\n"

#: bin/main.cc:112
msgid "      --no-options      start without restoring options\n"
msgstr "      --no-options      オプションを再設定しないで起動する\n"

#: bin/main.cc:113
msgid "      --help            display this help and exit\n"
msgstr "      --help            このヘルプを表示して終了する\n"

#: bin/main.cc:114
msgid "      --version         output version information and exit\n"
msgstr "      --version         バージョン情報を出力して終了する\n"

#: bin/main.cc:116
msgid "Report bugs to <rglclock@lists.hypercore.co.jp>.\n"
msgstr ""
"バグを見付けたら <rglclock@lists.hypercore.co.jp> に報告してください。\n"

