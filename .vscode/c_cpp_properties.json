{
    "version": 4,
    "configurations": [
        {
            "name": "POSIX x86_64 (GCC)",
            "compilerPath": "/usr/bin/gcc",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/programs",
                "${workspaceFolder}/lib",
                "${workspaceFolder}/libgnu",
                "/usr/include/gtk-3.0",
                "/usr/include/gdk-pixbuf-2.0",
                "/usr/include/pango-1.0",
                "/usr/include/atk-1.0",
                "/usr/include/glib-2.0",
                "/usr/lib/x86_64-linux-gnu/glib-2.0/include",
                "/usr/include/harfbuzz",
                "/usr/include/cairo"
            ],
            "defines": [
                "HAVE_CONFIG_H"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}
